# Uncomment this line to define a global platform for your project
# platform :ios, '9.0'

target 'cloub' do
  # Comment this line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!
  platform :ios, '8.0'

  # Pods for cloub
  pod 'Firebase/Auth'
  pod 'GoogleSignIn'
  pod 'GoogleMaps'
  pod 'GooglePlaces'
  pod 'Firebase/Database'
  pod 'SDWebImage', '~>3.8'
  pod 'Backendless'
  pod 'Firebase/Storage'

  target 'cloubTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'cloubUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
